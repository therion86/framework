{
  "autoload": {
    "psr-4": {
      "Framework\\": "lib",
      "App\\": "app"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests"
    }
  },
  "name": "therion/framework",
  "description": "A small Framework",
  "license": "MIT",
  "authors": [
    {
      "name": "Therion86"
    }
  ],
  "scripts": {
    "test" : "vendor/bin/phpunit phpunit.xml"
  },
  "config": {
    "platform": {
      "php": "8.1"
    }
  },
  "require": {
    "php": ">=8.1",
    "phpunit/phpunit": "^9.6",
    "phpspec/prophecy-phpunit": "^1.1.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest"
  }
}
